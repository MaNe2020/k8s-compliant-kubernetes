                    },
                    {
                        "IpProtocol": "TCP",
                        "FromPort": "7000",
                        "ToPort": "7000",
                        "CidrIp": "0.0.0.0/0"
                    }
                ]
            },
            "Metadata": {
                "AWS::CloudFormation::Designer": {
                    "id": "520f742e-d94f-43ed-9722-984b5ae5dcf7"
                }
